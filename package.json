{
  "name": "digikala",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass scss/main.scss public/css/style.css -w",
    "compile:sass": "sass scss/main.scss public/css/style.compile.css ",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 version' public/css/style.compile.css -o public/css/style.prefix.css",
    "compress:css": "sass public/css/style.prefix.css public/css/style.min.css --style compressed",
    "build:sass": "npm-run-all compile:sass prefix:css compress:css "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "bootstrap": "^5.3.7",
    "jquery": "^3.7.1",
    "swiper": "^11.2.10"
  }
}
